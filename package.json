{
  "name": "nl-tools-ui",
  "sideEffects": false,
  "version": "2.6.2",
  "description": "A system to automatically ingest data and tag",
  "main": "index.js",
  "scripts": {
    "---------------- Build": "",
    "webpack": "webpack-cli --config ./conf/webpack.config.js",
    "webpack:stats": "rimraf ./webpackStats.json && webpack-cli --config ./conf/webpack.config.js --stats --json > ./webpackStats.json",
    "webpack:analyze": "webpack-bundle-analyzer ./webpackStats.json",
    "sourcemap:explore": "source-map-explorer",
    "---------------- Dev Run Scripts": "",
    "backend": "java -Dspring.profiles.active=development -Delastic.properties=build/resources/main/elasticsearch-development.properties -Dlog4j.configurationFile=build/resources/main/log4j2.xml -jar build/libs/nl-tools-ui-2.6.2.jar",
    "webpack-dev-server": "webpack-dev-server --config ./conf/webpack.dev.server.config.js",
    "---------------- Linters": "",
    "eslint": "eslint --config=./conf/.eslintrc.json 'src/main/webapp/js/**/*.js' 'src/main/webapp/js/**/*.jsx' 'conf/*.js'",
    "eslint:fix": "eslint --fix --config=./conf/.eslintrc.json 'src/main/webapp/js/**/*.js' 'src/main/webapp/js/**/*.jsx' 'conf/*.js'",
    "eslint-win": "eslint --fix --config=./conf/.eslintrc.json src/main/webapp/js/**/*.js src/main/webapp/js/**/*.jsx conf/*.js",
    "eslint-win:fix": "eslint --fix --config=./conf/.eslintrc.json src/main/webapp/js/**/*.js src/main/webapp/js/**/*.jsx conf/*.js",
    "eslint:one": "eslint --fix --config=./conf/.eslintrc.json",
    "stylelint": "stylelint --syntax scss './src/main/webapp/**/*.scss'",
    "stylelint:one": "stylelint --syntax scss",
    "stylelint:fix": "stylelint --syntax scss './src/main/webapp/**/*.scss' --fix",
    "---------------- Test": "",
    "test": "cross-env NODE_ENV=test jest --coverage --verbose",
    "test:update": "cross-env NODE_ENV=test jest --updateSnapshot --coverage --verbose",
    "test:nocoverage": "cross-env NODE_ENV=test jest --verbose",
    "test:detectOpenHandles": "cross-env NODE_ENV=test jest --verbose --detectOpenHandles",
    "---------------- Clean": "",
    "clean": "cross-env rimraf dist node_modules coverage .nyc_output webpackStats.json"
  },
